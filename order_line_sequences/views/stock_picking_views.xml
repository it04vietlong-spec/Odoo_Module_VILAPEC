<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="view_picking_form" model="ir.ui.view">
        <field name="name">stock.picking.view.form.inherit.order.line.sequences</field>
        <field name="model">stock.picking</field>
        <field name="inherit_id" ref="stock.view_picking_form"/>
        <field name="priority">1000</field>
        <field name="arch" type="xml">
            <xpath expr="//sheet" position="inside">
                <field name="sequence_cleanup_done" invisible="1"/>
            </xpath>
            <xpath expr="//field[@name='move_ids_without_package']//field[@name='product_id']" position="before">
                <field name="sequence_number" string="#" readonly="1"/>
            </xpath>
        </field>
    </record>
</odoo>
